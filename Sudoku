    {
      "cell_type": "code",
      "source": [
        "# SUDOKU 9x9 - RESOLUÇÃO COM BACKTRACKING\n",
        "\n",
        "# Função para imprimir o tabuleiro 9x9\n",
        "def imprimir(tabuleiro):\n",
        "    for i in range(9):\n",
        "        if i % 3 == 0 and i != 0:\n",
        "            print(\"-\" * 21)\n",
        "\n",
        "        for j in range(9):\n",
        "            if j % 3 == 0 and j != 0:\n",
        "                print(\"|\", end=\" \")\n",
        "\n",
        "            print(tabuleiro[i][j], end=\" \")\n",
        "\n",
        "        print()\n",
        "\n",
        "# Função para encontrar posição vazia (valor 0)\n",
        "def encontrar_vazio(tabuleiro):\n",
        "    for i in range(9):\n",
        "        for j in range(9):\n",
        "            if tabuleiro[i][j] == 0:\n",
        "                return i, j\n",
        "    return None\n",
        "\n",
        "# Função que verifica se o número pode ser colocado\n",
        "def valido(tabuleiro, numero, posicao):\n",
        "    linha, coluna = posicao\n",
        "\n",
        "    # Verifica linha\n",
        "    for j in range(9):\n",
        "        if tabuleiro[linha][j] == numero and j != coluna:\n",
        "            return False\n",
        "\n",
        "    # Verifica coluna\n",
        "    for i in range(9):\n",
        "        if tabuleiro[i][coluna] == numero and i != linha:\n",
        "            return False\n",
        "\n",
        "    # Verifica bloco 3x3\n",
        "    bloco_linha = linha // 3\n",
        "    bloco_coluna = coluna // 3\n",
        "\n",
        "    for i in range(bloco_linha * 3, bloco_linha * 3 + 3):\n",
        "        for j in range(bloco_coluna * 3, bloco_coluna * 3 + 3):\n",
        "            if tabuleiro[i][j] == numero and (i, j) != posicao:\n",
        "                return False\n",
        "\n",
        "    return True\n",
        "\n",
        "# Função principal de resolução\n",
        "def resolver(tabuleiro):\n",
        "    vazio = encontrar_vazio(tabuleiro)\n",
        "\n",
        "    if not vazio:\n",
        "        return True  # Resolvido\n",
        "\n",
        "    linha, coluna = vazio\n",
        "\n",
        "    for numero in range(1, 10):\n",
        "        if valido(tabuleiro, numero, (linha, coluna)):\n",
        "            tabuleiro[linha][coluna] = numero\n",
        "\n",
        "            if resolver(tabuleiro):\n",
        "                return True\n",
        "\n",
        "            # Backtracking\n",
        "            tabuleiro[linha][coluna] = 0\n",
        "\n",
        "    return False\n",
        "\n",
        "# EXEMPLO DE SUDOKU 9x9\n",
        "\n",
        "tabuleiro = [\n",
        "    [5, 3, 0, 0, 7, 0, 0, 0, 0],\n",
        "    [6, 0, 0, 1, 9, 5, 0, 0, 0],\n",
        "    [0, 9, 8, 0, 0, 0, 0, 6, 0],\n",
        "    [8, 0, 0, 0, 6, 0, 0, 0, 3],\n",
        "    [4, 0, 0, 8, 0, 3, 0, 0, 1],\n",
        "    [7, 0, 0, 0, 2, 0, 0, 0, 6],\n",
        "    [0, 6, 0, 0, 0, 0, 2, 8, 0],\n",
        "    [0, 0, 0, 4, 1, 9, 0, 0, 5],\n",
        "    [0, 0, 0, 0, 8, 0, 0, 7, 9]\n",
        "]\n",
        "\n",
        "print(\"Sudoku inicial:\\n\")\n",
        "imprimir(tabuleiro)\n",
        "\n",
        "if resolver(tabuleiro):\n",
        "    print(\"\\nSudoku resolvido:\\n\")\n",
        "    imprimir(tabuleiro)\n",
        "else:\n",
        "    print(\"Não existe solução.\")"
      ],
      "metadata": {
